<component name="ProjectRunConfigurationManager">
  <configuration default="false" name="Eureka-Server" type="SpringBootApplicationConfigurationType" factoryName="Spring Boot">
    <module name="webdocs-eureka" />
    <extension name="coverage">
      <pattern>
        <option name="PATTERN" value="com.sensiblemetrics.api.ws.document.generator.*" />
        <option name="ENABLED" value="true" />
      </pattern>
    </extension>
    <option name="SPRING_BOOT_MAIN_CLASS" value="com.sensiblemetrics.api.ws.eureka.ApplicationBootstrap" />
    <option name="ENABLE_LAUNCH_OPTIMIZATION" value="false" />
    <option name="ENABLE_JMX_AGENT" value="false" />
    <option name="VM_PARAMETERS" value="-Dcom.sun.security.enableAIAcaIssuers=true" />
    <option name="ALTERNATIVE_JRE_PATH_ENABLED" value="true" />
    <option name="ALTERNATIVE_JRE_PATH" value="Java-11" />
    <option name="SHORTEN_COMMAND_LINE" value="MANIFEST" />
    <envs>
      <env name="JASYPT_ENCRYPTOR_PASSWORD" value="the_password" />
    </envs>
    <method v="2">
      <option name="Make" enabled="true" />
    </method>
  </configuration>
</component>